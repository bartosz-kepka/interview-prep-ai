---
import Layout from '../layouts/Layout.astro';

export const prerender = false;

// This page handles OAuth callbacks and email verification redirects from Supabase
// The actual session exchange will be handled by the backend in the future
---

<Layout title="Processing - Interview Prep AI">
  <main class="container mx-auto px-4 py-8 min-h-screen flex items-center justify-center">
    <div class="text-center space-y-4">
      <h1 class="text-2xl font-bold">Processing...</h1>
      <p class="text-muted-foreground">Please wait while we verify your account.</p>
    </div>
  </main>
</Layout>

